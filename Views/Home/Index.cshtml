@using TestWebApplication.Selenium;
@{
    ViewData["Title"] = "Home Page";
}

@inject WeatherFinder weatherFinder

<div>
    @foreach (Weather weather in weatherFinder.GetWeather())
    {
        <div>
            @weather.Day
        </div>
        <div>
            @weather.TempatureMax
        </div>
        <div>
            @weather.TempatureMin
        </div>

    }
</div>
